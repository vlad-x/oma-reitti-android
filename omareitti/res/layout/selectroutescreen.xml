<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content" android:orientation="vertical">
  
  <TableLayout android:id="@+id/address_row" android:layout_alignParentTop="true" android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="vertical" android:stretchColumns="*">    
  <TableRow android:id="@+id/SelectRouteScreenRow1" android:layout_width="match_parent" android:layout_height="wrap_content">
  	<TextView android:id="@+id/SelectRouteScreenTextFrom" android:textColor="#FF000000" android:layout_height="wrap_content" android:text="Address1" android:textStyle="normal|bold" android:gravity="top|left" android:layout_width="match_parent"></TextView>
  	<TextView android:id="@+id/SelectRouteScreenTextTo" android:textColor="#FF000000" android:layout_column="3" android:layout_height="wrap_content" android:text="Address2" android:textStyle="normal|bold" android:layout_width="match_parent" android:gravity="right" android:layout_gravity="right"></TextView>
  </TableRow>
  </TableLayout>
  
  <TableLayout android:id="@+id/address_row1" android:layout_below="@+id/address_row" android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="vertical" android:stretchColumns="*">
  <TableRow android:id="@+id/SelectRouteScreenRow3" android:layout_width="match_parent" android:layout_height="wrap_content">
  	<TextView android:id="@+id/SelectRouteScreenTextTimeType" android:textColor="#FF000000" android:layout_height="wrap_content" android:text="Address1" android:textStyle="normal" android:gravity="top|left" android:layout_width="match_parent"></TextView>
  	<TextView android:text="" android:id="@+id/SelectRouteScreenTextIcons" android:layout_column="2" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
  	<TextView android:id="@+id/SelectRouteScreenTextOptimization" android:textColor="#FF000000" android:layout_column="3" android:layout_height="wrap_content" android:text="Address2" android:textStyle="normal" android:layout_width="match_parent" android:gravity="right" android:layout_gravity="right"></TextView>
  </TableRow>
  </TableLayout>
   
  <TableLayout android:id="@+id/routes_row" android:layout_below="@+id/address_row1" android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="vertical" android:stretchColumns="*">    
  <TableRow android:id="@+id/SelectRouteScreenRow2" android:layout_width="match_parent" android:layout_height="wrap_content">
      <ListView android:layout_height="wrap_content" android:id="@+id/SelectRouteScreenListView" android:layout_width="match_parent"></ListView>
  </TableRow>
  </TableLayout>
  
</RelativeLayout>
